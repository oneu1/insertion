function sortedArray = insertionSort(arr)
    n = length(arr);
    for i = 2:n
        key = arr(i);
        j = i - 1;
        while j > 0 & arr(j) > key
            arr(j + 1) = arr(j);
            j = j - 1;
        end
        arr(j + 1) = key;
    end
    sortedArray = arr;
endfunction

arr = [12, 11, 13, 5, 6];
sortedArr = insertionSort(arr);
disp("Sorted Array: ");
disp(sortedArr);
